#include<bits/stdc++.h>
using namespace std;
void swap(string a,string b)
{
    string temp=a;
    a=b;
    b=temp;
}
string swapString(string s)
{
    int i,j;
    char t;
    for(i=0,j=s.length()-1;i<j;i++,j--)
    {
        swap(s[i],s[j]);
    }
    return s;
}
int main()
